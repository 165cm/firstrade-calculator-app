{
  "name": "firstrade-calculator-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:job": "tsc -p tsconfig.job.json",
    "start:job": "NODE_OPTIONS='--experimental-vm-modules' node dist/services/exchangeService/job.js",
    "dev:job": "ts-node --esm src/services/exchangeService/job.ts",
    "check-rates": "NODE_OPTIONS='--loader ts-node/esm' ts-node --esm scripts/checkStoredRates.ts",
    "update-rates": "NODE_OPTIONS='--loader ts-node/esm' ts-node --esm scripts/updateExchangeRates.ts",
    "process-rates": "NODE_OPTIONS='--loader ts-node/esm' ts-node --esm scripts/processRates.ts",
    "fetch-rates": "NODE_OPTIONS='--loader ts-node/esm' ts-node --esm scripts/fetchHistoricalRates.ts",
    "generate-rates": "NODE_OPTIONS='--loader ts-node/esm' ts-node --project tsconfig.scripts.json scripts/generateHistoricalRates.ts"
  },
  "dependencies": {
    "@google-cloud/logging": "^11.2.0",
    "@google-cloud/pubsub": "^4.9.0",
    "@radix-ui/react-dialog": "^1.1.4",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.473.0",
    "next": "^15.1.5",
    "papaparse": "^5.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "recharts": "^2.12.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@swc/core": "^1.10.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/next": "^8.0.7",
    "@types/node": "^20.17.14",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.5",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
}
